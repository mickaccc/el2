                <GradientStop Color="DarkOrange" Offset="0.122" />
                <GradientStop Color="Black" Offset="0" />
                <GradientStop Color="#FF7B7B7B" Offset="0.308" />
                <GradientStop Color="#FFB6DF59" Offset="0.715" />
