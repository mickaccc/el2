<Window x:Class="Lieferliste_WPF.Views.PrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lieferliste_WPF.Views"
        mc:Ignorable="d"
        Title="PrintWindow" MinHeight="450" MinWidth="800">
    <Grid>
        <DocumentViewer  x:Name="PreviewD"  IsManipulationEnabled="True" >
            <DocumentViewer.LayoutTransform>
                <TransformGroup>
                    <ScaleTransform  />
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </DocumentViewer.LayoutTransform>

        </DocumentViewer>
 
    </Grid>
</Window>
