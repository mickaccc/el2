<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lieferliste_WPF.View"
        xmlns:ViewModels="clr-namespace:Lieferliste_WPF.ViewModels" x:Class="Lieferliste_WPF.View.OrderView"
        mc:Ignorable="d"
        Title="OrderView" Height="450" Width="800">
    <Grid>

        <ListView d:ItemsSource="{d:SampleData ItemCount=5}" ItemsSource="{Binding Users}" IsEnabled="False">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
